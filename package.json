        "jquery": "^3.1.1",
        "laravel-mix": "^1.5.1",
        "lodash": "^4.17.4",
        "preload-webpack-plugin": "^2.0.0",
        "vue": "^2.5.2"
    },
    "dependencies": {
        "@fengyuanchen/datepicker": "^0.6.3",
        "@fortawesome/fontawesome": "^1.0.1",
        "@fortawesome/fontawesome-pro-light": "^5.0.1",
