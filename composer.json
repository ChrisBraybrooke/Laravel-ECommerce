{
  "name": "chrisbraybrooke/laravel-ecommerce",
  "description": "A laravel ecommerce solution.",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Braybrooke",
      "email": "chris@purplemountmedia.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0",
    "laravel/framework": "5.6.*",
    "spatie/laravel-permission": "^2.7",
    "spatie/laravel-activitylog": "^2.3",
    "spatie/laravel-medialibrary": "^6.0.0",
    "anlutro/l4-settings": "^0.6.0",
    "olssonm/l5-zxcvbn": "^3.1",
    "stripe/stripe-php": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "ChrisBraybrooke\\ECommerce\\": "src/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "ChrisBraybrooke\\ECommerce\\ECommerceServiceProvider"
          ],
          "aliases": {
              "Shop": "ChrisBraybrooke\\ECommerce\\Shop"
          }
      }
  }
}
